public class BasicArray {
  public BasicArray() {}

  public static int test() {
    int[] as = new int[3];
    as[0] = 0;
    as[1] = 1;
    as[2] = 2;
    
    if (as.length != 3) return as.length;
    if (as[0] != 0) return as[0];
    if (as[1] != 1) return as[1];
    if (as[2] != 2) return as[2];
    return 123;
  }
}
